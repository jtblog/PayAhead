{
  "rules": {
    ".read": "true",
    ".write": "true",

    "industry": {
      ".read": "true",
      ".write": "true"
    },
    "users": {
      "$uid": {
        ".read": "true",
        ".write": "$uid === newData.child('uid').val()"
      }
    },
    /*"reports": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },*/
    "authorization": {
      ".read": "true",
      ".write": "true"
    },
    "paystack": {
      ".read": "true",
      ".write": "false",
      "keys": {
        ".read": "true",
        ".write": "false"
      }
    },
    "logs":{
      ".read": "true",
      ".write": "true"
    },
    "payments": {
      "$reference": {
        ".read": "$uid === auth.uid",
        ".write": "newData.hasChildren(['payer_id', 'payee_id', epoch])"
      }
    }
  }
}